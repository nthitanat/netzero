# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Tree API Configuration
# Set to 'true' to use real API endpoints
# Set to 'false' or leave unset to use mock data for development
REACT_APP_USE_REAL_TREE_API=false

# Development Settings
NODE_ENV=development

# Optional: Tree Image CDN Base URL
REACT_APP_TREE_IMAGES_BASE_URL=https://your-cdn.com/tree-images

# Optional: Enable API Request Logging
REACT_APP_ENABLE_API_LOGGING=true
